<h2>Welcome</h2>
<H2>{{user.name}}</H2>


<a href="/profile/edit" rel="noopener noreferrer">Edit</a>
<a href="{{user.id}}/edit/gameList" rel="noopener noreferrer">Game List</a>
<hr>
<h3>Game List</h3>
{{#each user.gameList}}
  <a href="/game/{{id}}">
  <img src="{{background_image}}" alt="" style= 'width: 250px;'>
  <p>{{name}}</p>
  </a>
{{/each}}
<hr>
<h3>Wishlist</h3>
{{#each user.wishlist}}
  <a href="/game/{{id}}">
  <img src="{{background_image}}" alt="" style= 'width: 250px;'>
  <p>{{name}}</p>
  </a>
{{/each}}






